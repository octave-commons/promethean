{
  "name": "reasoner",
  "version": "0.1.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && node scripts/patch-imports.js",
    "build:check": "tsc --noEmit --incremental false",
    "test": "npm run build && ava",
    "coverage": "npm run build && c8 ava",
    "lint": "biome lint .",
    "format": "biome format --write ."
  },
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.10",
    "@types/supertest": "^2.0.13",
    "@biomejs/biome": "^2.1.3",
    "ava": "^6.4.1",
    "c8": "^9.1.0",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3"
  },
  "ava": {
    "files": [
      "dist/tests/**/*.js"
    ]
  }
}
