{
  "name": "ts-<%= name %>",
  "root": "packages/<%= name %>",
  "sourceRoot": "packages/<%= name %>/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -b",
        "cwd": "packages/<%= name %>"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tsconfig.json --noEmit",
        "cwd": "packages/<%= name %>"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ava",
        "cwd": "packages/<%= name %>"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint .",
        "cwd": "packages/<%= name %>"
      }
    }
  },
  "tags": ["scope:packages"]
}
