{
  "name": "@promethean/discord-embedder",
  "version": "0.0.1",
  "description": "A proof-of-concept basic bot using @discordjs/voice",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && node scripts/patch-imports.js",
    "test": "pnpm run build && ava --config ../../../config/ava.config.mjs",
    "coverage": "pnpm run build && c8 ava --config ../../../config/ava.config.mjs",
    "build:check": "tsc --noEmit --incremental false -p .",
    "deploy": "pnpm run build && node --env-file=../../.env dist/util/deploy.js",
    "lint": "pnpm exec biome lint --config-path ../config/biome.json .. || true",
    "format": "pnpm exec biome format --config-path ../config/biome.json ..",
    "start": "pnpm run build && node --env-file=../../.env dist/index.js",
    "start:dev": "node --env-file=../../.env --loader ts-node/esm  src/index.ts"
  },
  "keywords": [
    "discord",
    "radio",
    "bot",
    "audio",
    "speakers",
    "hardware",
    "dj"
  ],
  "author": "Amish Shah <amishshah.2k@gmail.com>",
  "license": "UNLICENSE",
  "dependencies": {
    "@discordjs/voice": "^0.18.0",
    "@shared/js": "file:../../js",
    "@promethean/embeddings": "file:../embeddings",
    "@promethean/persistence": "file:../persistence",
    "chromadb": "^3.0.9",
    "discord.js": "^14.17.3",
    "dotenv": "^17.2.0",
    "libsodium-wrappers": "^0.7.13",
    "mongodb": "^6.17.0",
    "node-crc": "^1.3.2",
    "ollama": "^0.5.16",
    "pnpm": "^10.13.1",
    "prism-media": "^2.0.0-alpha.0",
    "wav": "^1.0.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^22.17.2",
    "@types/ws": "^8.5.12",
    "ava": "^6.4.1",
    "c8": "^9.1.0",
    "@biomejs/biome": "^2.1.4",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3"
  }
}
