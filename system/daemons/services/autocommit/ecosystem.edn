{:apps [{:id "autocommit"
          :name "@promethean/autocommit"
          :script "autocommit"
          :cwd "/home/err/devel/promethean"
          :args ["--path" "."
                 "--debounce-ms" "10000"
                 "--model" "llama3.1:8b"]
          :env {:OPENAI_BASE_URL "http://localhost:11434/v1"
                :AUTOCOMMIT_MODEL "error/qwen3:4b-instruct-100k"
