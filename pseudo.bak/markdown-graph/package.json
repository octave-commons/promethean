{
  "name": "@promethean/markdown-graph",
  "version": "0.1.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc ",
    "build:check": "tsc --noEmit --incremental false",
    "test": "pnpm run build && ava --config ../../../config/ava.config.mjs",
    "coverage": "pnpm run build && c8 ava --config ../../../config/ava.config.mjs",
    "lint": "prettier --cache --check . && eslint src --cache",
    "format": "prettier --cache --write . && eslint src --fix --cache",
    "start": "node dist/index.js",
    "start:dev": "node --loader ts-node/esm src/index.ts"
  },
  "dependencies": {
    "@promethean/persistence": "workspace:*",
    "@types/body-parser": "^1.19.5",
    "body-parser": "^1.20.3",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@promethean/test-utils": "workspace:*",
    "@types/express": "^4.17.21",
    "@types/supertest": "^2.0.13",
    "eslint": "^8.57.0",
    "eslint-config-neon": "^0.1.62",
    "mongodb-memory-server": "^10.1.4",
    "prettier": "^3.4.2",
    "supertest": "^7.1.1",
    "@promethean/pm2-helpers": "workspace:*"
  }
}
