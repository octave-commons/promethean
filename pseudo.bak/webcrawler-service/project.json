{
  "name": "@promethean/webcrawler-service",
  "root": "pseudo/webcrawler-service",
  "sourceRoot": "pseudo/webcrawler-service/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -b",
        "cwd": "pseudo/webcrawler-service"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tsconfig.json --noEmit",
        "cwd": "pseudo/webcrawler-service"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "pnpm exec ava --config ../../config/ava.config.mjs",
        "cwd": "{projectRoot}",
        "env": {
          "AVA_PROJECT_ROOT": "."
        }
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint .",
        "cwd": "pseudo/webcrawler-service"
      }
    }
  },
  "tags": ["scope:packages"]
}
