{:source-paths ["src/chat-ui"
                 "src/chat-ui/cljs"
                 "src/docops/cljs"
                 "src/duck-web/cljs"
                 "src/kanban/cljs"
                 "src/main/cljs"
                 "src/openai-server/cljs"
                 "src/opencode-session-manager/cljs"
                 "src/piper/cljs"
                 "src/report-forge/cljs"
                 "src/smartgpt-dashboard/cljs"
                 "../shadow-ui/src"]
 :dependencies [[reagent "1.2.0"]
                [re-frame "1.3.0"]
                [day8.re-frame/re-frame-10x "1.9.8"]
                [day8.re-frame/tracing "0.6.2"]
                [day8.re-frame/tracing-stubs "0.6.2"]]
 :builds
 {:main {:target :browser
         :output-dir "dist/main"
         :asset-path "/js"
         ;; Downstream surfaces are temporarily represented by placeholders so we
         ;; keep a single :main module to avoid cross-module dependency errors.
         :modules {:main {:init-fn promethean.main.app/init
                          :default true}}}}
 :devtools {:http-root "static"
            :http-port 3000
            :http-resource-root "/"
            :preloads [devtools.preload
                       day8.re-frame-10x.preload]}
 :dev {:compiler-options {:closure-defines {re-frame.trace/trace-enabled? true
                                            day8.re-frame.tracing/trace-enabled? true}}}}
