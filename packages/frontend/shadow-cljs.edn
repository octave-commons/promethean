{:source-paths ["src"]
 :dependencies [[reagent "1.2.0"]
                   [re-frame "1.3.0"]
                   [day8.re-frame/re-frame-10x "1.9.8"]
                   [day8.re-frame/tracing "0.6.2"]
                   [day8.re-frame/tracing-stubs "0.6.2"]]
 :builds
 {:main {:target :browser
         :output-dir "dist/main"
         :asset-path "/js"
         :modules {:main {:init-fn promethean.main.app/init}
                   :chat-ui {:entries [promethean.frontends.chat-ui.app]}
                   :docops {:entries [promethean.frontends.docops.app]}
                   :duck-web {:entries [promethean.frontends.duck-web.app]}
                   :kanban {:entries [core]}
                   :openai-server {:entries [promethean.frontends.openai-server.app]}
                   :opencode-session {:entries [promethean.frontends.opencode-session-manager.app]}
                   :piper {:entries [promethean.frontends.piper.app]}
                   :report-forge {:entries [promethean.frontends.report-forge.app]}
                   :smartgpt-dashboard {:entries [promethean.frontends.smartgpt-dashboard.app]}}
         :devtools {:http-root "static"
                    :http-port 3000
                    :http-resource-root "/"
                    :preloads [devtools.preload
                               day8.re-frame-10x.preload]}
         :dev {:compiler-options {:closure-defines {re-frame.trace/trace-enabled? true
                                                   day8.re-frame.tracing/trace-enabled? true}}}}}