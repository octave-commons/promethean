{
  "include": ["dist/**/*.js"],
  "exclude": [
    "dist/tests/**/*.js",
    "dist/**/*.test.js",
    "dist/**/*.d.ts",
    "src/**",
    "coverage/**",
    "**/*.config.*",
    "node_modules/**"
  ],
  "reporter": ["text", "html", "lcov", "json"],
  "reportsDir": "coverage",
  "branches": 80,
  "functions": 80,
  "lines": 80,
  "statements": 80,
  "clean": true,
  "checkCoverage": false,
  "tempDirectory": ".nyc_output",
  "extension": [".js"],
  "skipFull": false,
  "all": true,
  "src": ["dist/"]
}
