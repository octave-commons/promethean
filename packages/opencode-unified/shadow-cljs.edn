{:source-paths ["src/clojurescript" "src"]
 :dependencies [[reagent "1.2.0"]
                 [re-frame "1.3.0"]
                 [day8.re-frame/re-frame-10x "1.9.8"]
                 [day8.re-frame/tracing "0.6.2"]
                 [day8.re-frame/undo "0.3.3"]
                 [cljsjs/react "18.2.0"]
                 [cljsjs/react-dom "18.2.0"]
                 [cider/cider-nrepl "0.28.7"]
                 [refactor-nrepl "3.9.1"]
                 [clj-commons/pomegranate "1.1.0"]
                [org.clojure/core.async "1.6.673"]
                [org.clojure/tools.logging "1.2.4"]
                [org.clojure/data.json "2.4.0"]
                [com.taoensso/timbre "6.3.1"]
                [com.taoensso/tempura "1.3.0"]
                [thheller/shadow-cljs "2.28.23"]
                [binaryage/devtools "1.0.7"]
                [day8.re-frame/http-fx "0.2.4"]
                [cljs-http "0.1.46"]
                [metosin/reitit "0.7.0"]
                [metosin/reitit-frontend "0.7.0"]
                [org.clojure/core.match "1.0.1"]
                [org.clojure/spec.alpha "0.3.218"]
                [expound "0.9.0"]]

 :builds
 {:app {:target :browser
        :output-dir "dist/public/js"
        :asset-path "/js"
        :modules {:main {:init-fn opencode.app/init
                        :preloads [devtools.preload
                                   day8.re-frame-10x.preload]}}
        :devtools {:http-root "dist/public"
                   :http-port 8080
                   :preloads [devtools.preload
                              day8.re-frame-10x.preload]}
        :dev {:compiler-options {:closure-defines {re-frame.trace/trace-enabled? true
                                                  day8.re-frame.tracing/trace-enabled? true}}}
        :release {:build-options {:ns-aliases {opencode.app opencode.app}}
                  :compiler-options {:optimizations :advanced
                                    :infer-externs :auto}}}

  :renderer {:target :electron-renderer
             :output-dir "dist/renderer"
             :modules {:main {:init-fn opencode.renderer/init}}
             :devtools {:http-root "dist/renderer"
                        :http-port 8081}
             :dev {:compiler-options {:closure-defines {re-frame.trace/trace-enabled? true
                                                        day8.re-frame.tracing/trace-enabled? true}}}
             :release {:compiler-options {:optimizations :advanced}}}

  :preload {:target :electron-renderer
            :output-dir "dist/preload"
            :modules {:main {:init-fn opencode.preload/init}}
            :release {:compiler-options {:optimizations :simple}}}

  :main {:target :node-script
         :output-to "dist/main.js"
         :main opencode.main/main
         :release {:compiler-options {:optimizations :simple}}}

  :server {:target :node-script
           :output-to "dist/server.js"
           :main opencode.server/main
           :release {:compiler-options {:optimizations :simple}}}

  :test {:target :node-test
          :output-to "dist/test.js"
          :ns-regexp "test\\..*"
          :autorun true}}}