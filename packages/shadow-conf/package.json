{
  "name": "@promethean/shadow-conf",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./bin": {
      "default": "./dist/bin/shadow-conf.js"
    },
    "./dist/*": "./dist/*",
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "license": "GPL-3.0-only",
  "bin": {
    "shadow-conf": "./dist/bin/shadow-conf.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm run build && ava --config ../../config/ava.config.mjs",
    "lint": "pnpm exec eslint .",
    "coverage": "pnpm run build && c8 ava --config ../../config/ava.config.mjs",
    "format": "pnpm exec prettier --write ."
  },
  "dependencies": {
    "dotenv": "17.2.1",
    "@promethean/pm2-helpers": "workspace:*",
    "jsedn": "0.4.1"
  },
  "devDependencies": {}
}
