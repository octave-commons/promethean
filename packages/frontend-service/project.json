{
  "name": "@promethean/frontend-service",
  "root": "packages/frontend-service",
  "sourceRoot": "packages/frontend-service/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -b",
        "cwd": "{projectRoot}"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tsconfig.json --noEmit"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "ava",
        "env": {
          "AVA_PROJECT_ROOT": "."
        },
        "cwd": "{projectRoot}"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint ."
      }
    }
  },
  "tags": ["scope:packages"]
}
