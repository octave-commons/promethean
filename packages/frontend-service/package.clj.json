{
  "name": "@promethean/frontend-service-cljs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/frontend_service.cjs",
  "exports": {
    ".": {
      "import": "./dist/frontend_service.mjs",
      "require": "./dist/frontend_service.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "shadow-cljs compile frontend-service",
    "clean": "shadow-cljs clean frontend-service",
    "typecheck": "shadow-cljs check frontend-service",
    "test": "shadow-cljs compile frontend-service && node -e \"require('./dist/frontend_service.cjs')\"",
    "dev": "shadow-cljs watch frontend-service",
    "release": "shadow-cljs release frontend-service",
    "repl": "shadow-cljs cljs-repl frontend-service"
  },
  "dependencies": {
    "fastify": "5.3.2",
    "@fastify/static": "8.2.0",
    "@promethean/web-utils": "workspace:*"
  },
  "devDependencies": {
    "shadow-cljs": "^2.28.17",
    "thheller/shadow-cljs": "^2.28.17",
    "org.typedclojure/typed.cljs.core": "1.1.5",
    "org.typedclojure/typed.cljs.checker": "1.1.5",
    "org.typedclojure/typed.cljs.runtime": "1.1.5"
  },
  "shadow-cljs": {
    "source-paths": [
      "src/cljs"
    ],
    "dependencies": [
      [org.typedclojure/typed.cljs.core "1.1.5"]
    ],
    "compiler-options": {
      "infer-externs": false,
      "check-configs": {
        "org.typedclojure/typed.cljs.checker": {
          "check-configs": {
            "check": true
          }
        }
      }
    }
  }
}