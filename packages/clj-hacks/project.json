{
  "name": "ts-clj-hacks",
  "root": "packages/clj-hacks",
  "sourceRoot": "packages/clj-hacks/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bb clj-hacks:build"
      },
      "outputs": ["{projectRoot}/target"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bb clj-hacks:lint"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "clojure -M:test",
        "cwd": "packages/clj-hacks"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bb clj-hacks:lint"
      }
    }
  },
  "tags": ["scope:packages"]
}
