{
  "name": "@promethean/benchmark",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm run build && ava --config ../../config/ava.config.mjs",
    "lint": "pnpm exec eslint .",
    "coverage": "pnpm run build && c8 ava --config ../../config/ava.config.mjs",
    "format": "pnpm exec prettier --write .",
    "benchmark": "pnpm run build && node dist/cli.js",
    "benchmark:compare": "pnpm run build && node dist/cli.js --compare",
    "benchmark:health": "pnpm run build && node dist/cli.js --health",
    "benchmark:list": "pnpm run build && node dist/cli.js --list"
  },
  "module": "dist/index.js",
  "dependencies": {
    "@promethean/utils": "workspace:*",
    "ollama": "^0.5.17",
    "openai": "^5.23.2",
    "zod": "^3.23.8",
    "node-fetch": "^3.3.2",
    "systeminformation": "^5.23.5",
    "pidusage": "^3.0.2",
    "cli-table3": "^0.6.4",
    "chalk": "^5.3.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/node-fetch": "^2.6.9",
    "@types/pidusage": "^2.0.5"
  }
}
