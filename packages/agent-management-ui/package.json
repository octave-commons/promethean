{
  "name": "@promethean/agent-management-ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "static"
  ],
  "scripts": {
    "build": "tsc && pnpm run build:components",
    "build:components": "node scripts/build-components.js",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm run build && ava --config ../../config/ava.config.mjs",
    "lint": "pnpm exec eslint .",
    "coverage": "pnpm run build && c8 ava --config ../../config/ava.config.mjs",
    "format": "pnpm exec prettier --write .",
    "dev": "pnpm run build && node scripts/dev-server.js",
    "serve": "node dist/server.js"
  },
  "dependencies": {
    "@promethean/agents-workflow": "workspace:*",
    "@promethean/monitoring": "workspace:*",
    "@promethean/ui-components": "workspace:*",
    "@promethean/kanban": "workspace:*",
    "lit": "^3.1.0",
    "lit-element": "^4.0.0",
    "lit-html": "^3.1.0",
    "@lit-labs/ssr-client": "^1.0.0",
    "rxjs": "^7.8.0",
    "chart.js": "^4.4.0",
    "chartjs-adapter-luxon": "^1.3.0",
    "luxon": "^3.4.0",
    "marked": "5.0.2",
    "dompurify": "^3.0.0",
    "sortablejs": "^1.15.0",
    "virtual-scroll": "2.2.1"
  },
  "devDependencies": {
    "@types/sortablejs": "^1.15.0",
    "@types/dompurify": "^3.0.0",
    "@types/marked": "5.0.2",
    "esbuild": "^0.19.0",
    "express": "^4.19.0",
    "ws": "^8.18.0",
    "rimraf": "^6.0.0"
  },
  "license": "GPL-3.0-only"
}
