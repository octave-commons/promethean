#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/err/devel/promethean/node_modules/.pnpm/ava@6.4.1_@ava+typescript@4.1.0_encoding@0.1.13_rollup@4.52.3/node_modules/ava/entrypoints/node_modules:/home/err/devel/promethean/node_modules/.pnpm/ava@6.4.1_@ava+typescript@4.1.0_encoding@0.1.13_rollup@4.52.3/node_modules/ava/node_modules:/home/err/devel/promethean/node_modules/.pnpm/ava@6.4.1_@ava+typescript@4.1.0_encoding@0.1.13_rollup@4.52.3/node_modules:/home/err/devel/promethean/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/err/devel/promethean/node_modules/.pnpm/ava@6.4.1_@ava+typescript@4.1.0_encoding@0.1.13_rollup@4.52.3/node_modules/ava/entrypoints/node_modules:/home/err/devel/promethean/node_modules/.pnpm/ava@6.4.1_@ava+typescript@4.1.0_encoding@0.1.13_rollup@4.52.3/node_modules/ava/node_modules:/home/err/devel/promethean/node_modules/.pnpm/ava@6.4.1_@ava+typescript@4.1.0_encoding@0.1.13_rollup@4.52.3/node_modules:/home/err/devel/promethean/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ava/entrypoints/cli.mjs" "$@"
else
  exec node  "$basedir/../ava/entrypoints/cli.mjs" "$@"
fi
