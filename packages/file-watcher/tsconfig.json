{
  "extends": "../../config/tsconfig.base.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "composite": true,
    "declaration": true,
    "allowJs": true,
    "paths": {
      "@shared/ts/*": [
        "../../shared/ts/dist/*"
      ],
      "@promethean/test-utils/*": [
        "../../packages/test-utils/dist/*"
      ],
      "@promethean/legacy/*": [
        "../../packages/legacy/*"
      ]
    },
    "baseUrl": ".",
    "esModuleInterop": true,
    "downlevelIteration": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "references": []
}