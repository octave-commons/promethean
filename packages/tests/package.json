{
    "name": "@promethean/tests",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./dist/*": "./dist/*",
        "./*": "./dist/*"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "clean": "rimraf dist",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "test": "pnpm -r --filter @promethean/tests^... build && ava --config ../../config/ava.config.mjs",
        "lint": "pnpm exec eslint .",
        "lisp": "node dist/cli/lisp.js",
        "coverage": "pnpm -r --filter @promethean/tests^... run build && c8 --cwd ../../ --exclude-node-modules=false --all --include 'packages/**/*.js' --include 'packages/**/*.ts' --reporter=text --reporter=lcov --report-dir ../../coverage ava --config ../../config/ava.config.mjs",
        "test:markdown": "pnpm run build && ava --config ../../config/ava.config.mjs dist/tests/markdown.*.test.js",
        "format": "pnpm exec prettier --write ."
    },
    "module": "dist/index.js",
    "dependencies": {
        "@promethean/compiler": "workspace:*",
        "@promethean/dev": "workspace:*",
        "@promethean/fs": "workspace:*",
        "@promethean/markdown": "workspace:*",
        "@promethean/parity": "workspace:*",
        "@promethean/stream": "workspace:*",
        "@promethean/test-utils": "workspace:*",
        "@promethean/web-utils": "workspace:*",
        "@types/javascript-time-ago": "2.5.0",
        "@types/unist": "3.0.3",
        "body-parser": "1.20.3",
        "chromadb": "3.0.14",
        "duckduckgo-search": "1.0.7",
        "express": "4.21.2",
        "gray-matter": "4.0.3",
        "javascript-time-ago": "2.5.11",
        "mongodb": "6.18.0",
        "ollama": "0.5.17",
        "prom-client": "15.1.3",
        "remark-gfm": "4.0.1",
        "remark-parse": "11.0.0",
        "remark-stringify": "11.0.0",
        "sucrase": "3.35.0",
        "unified": "11.0.5",
        "unist": "0.0.1",
        "unist-util-to-string-with-nodes": "0.1.1",
        "unist-util-visit": "5.0.0",
        "uuid": "11.1.0",
        "ws": "8.18.3",
        "yaml": "2.8.1",
        "zod": "3.25.76"
    },
    "devDependencies": {
        "@types/body-parser": "1.19.5",
        "@types/estree": "1.0.5",
        "@types/express": "4.17.21",
        "@types/ws": "8.5.9"
    }
}
