{
  "name": "@promethean/dualstore-http",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm run build && ava --config ../../config/ava.config.mjs",
    "lint": "pnpm exec eslint .",
    "coverage": "pnpm run build && c8 ava --config ../../config/ava.config.mjs",
    "format": "pnpm exec prettier --write .",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "web-ui": "node src/web-ui/server.js"
  },
  "module": "dist/index.js",
  "dependencies": {
    "@promethean/persistence": "workspace:*",
    "fastify": "^5.0.0",
    "@fastify/cors": "^10.0.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/bearer-auth": "^9.4.0",
    "zod": "^3.25.76",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.11",
    "ava": "^6.4.1",
    "c8": "^10.1.3",
    "tsx": "^4.20.3"
  }
}
