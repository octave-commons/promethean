{
  "name": "@promethean/nlp-command-parser",
  "version": "1.0.0",
  "description": "Natural language command parser for converting conversational input to structured commands",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "test": "ava",
    "test:unit": "ava src/tests/unit/*.test.ts",
    "test:integration": "ava src/tests/integration/*.test.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "coverage": "c8 ava"
  },
  "keywords": [
    "nlp",
    "parser",
    "natural-language",
    "command-line",
    "cli",
    "kanban",
    "task-management"
  ],
  "author": "Promethean Team",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@promethean/kanban": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ava": "^6.0.0",
    "c8": "^9.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./patterns": {
      "types": "./dist/patterns.d.ts",
      "import": "./dist/patterns.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ]
}
