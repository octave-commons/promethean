#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/err/devel/promethean/node_modules/.pnpm/nbb@1.3.204/node_modules/nbb/node_modules:/home/err/devel/promethean/node_modules/.pnpm/nbb@1.3.204/node_modules:/home/err/devel/promethean/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/err/devel/promethean/node_modules/.pnpm/nbb@1.3.204/node_modules/nbb/node_modules:/home/err/devel/promethean/node_modules/.pnpm/nbb@1.3.204/node_modules:/home/err/devel/promethean/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/bun" ]; then
  exec "$basedir/bun"  "$basedir/../nbb/nbbun.js" "$@"
else
  exec bun  "$basedir/../nbb/nbbun.js" "$@"
fi
