{
  "name": "@promethean/kanban",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist/lib/*.js"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "kanban": "dist/index.js"
  },
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "test": "pnpm run build && ava"
  },
  "dependencies": {
    "@promethean/markdown": "workspace:*",
    "@promethean/utils": "workspace:*",
    "gray-matter": "^4.0.3",
    "mongodb": "^6.18.0",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0"
  }
}
