{
  "name": "@promethean/agent-generator",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "resources"
  ],
  "scripts": {
    "build": "shadow-cljs release agent-generator",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm run build && ava --config ../../config/ava.config.mjs",
    "lint": "pnpm exec eslint .",
    "coverage": "pnpm run build && c8 ava --config ../../config/ava.config.mjs",
    "format": "pnpm exec prettier --write .",
    "shadow:watch": "shadow-cljs watch agent-generator",
    "shadow:release": "shadow-cljs release agent-generator",
    "nbb:build": "nbb -cp src -m promethean.agent_generator.cli.core"
  },
  "module": "dist/index.js",
  "dependencies": {
    "@types/node": "^20.0.0",
    "cheshire": "0.1.42",
    "fs-extra": "^11.0.0",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "shadow-cljs": "^2.28.17",
    "nbb": "^1.3.182"
  },
  "license": "GPL-3.0-only"
}
