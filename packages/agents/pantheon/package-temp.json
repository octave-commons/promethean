{
  "name": "@promethean/pantheon",
  "version": "1.0.0",
  "description": "Promethean Pantheon - Unified Agent Framework consolidating context management, orchestration, protocols, and workflow systems",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./*": "./dist/*",
    "./context": {
      "types": "./dist/context/index.d.ts",
      "import": "./dist/context/index.js"
    },
    "./orchestrator": {
      "types": "./dist/orchestrator/index.d.ts",
      "import": "./dist/orchestrator/index.js"
    },
    "./protocol": {
      "types": "./dist/protocol/index.d.ts",
      "import": "./dist/protocol/index.js"
    },
    "./workflow": {
      "types": "./dist/workflow/index.d.ts",
      "import": "./dist/workflow/index.js"
    },
    "./os-protocol": {
      "types": "./dist/os-protocol/index.d.ts",
      "import": "./dist/os-protocol/index.js"
    },
    "./generator": {
      "types": "./dist/generator/index.d.ts",
      "import": "./dist/generator/index.js"
    },
    "./management-ui": {
      "types": "./dist/management-ui/index.d.ts",
      "import": "./dist/management-ui/index.js"
    }
  },
  "files": [
    "dist",
    "resources",
    "static"
  ],
  "scripts": {
    "build": "tsc && shadow-cljs release pantheon",
    "build:ts": "tsc",
    "build:clj": "shadow-cljs release pantheon",
    "clean": "rimraf dist out",
    "dev": "tsc --watch",
    "dev:clj": "shadow-cljs watch pantheon",
    "typecheck": "tsc --noEmit",
    "test": "ava",
    "test:unit": "ava --match='*unit*'",
    "test:integration": "ava --match='*integration*'",
    "test:e2e": "ava --match='*e2e*'",
    "lint": "pnpm exec eslint .",
    "lint:fix": "pnpm exec eslint . --fix",
    "coverage": "c8 ava",
    "format": "pnpm exec prettier --write .",
    "shadow:watch": "shadow-cljs watch pantheon",
    "shadow:release": "shadow-cljs release pantheon",
    "nbb:build": "nbb -cp src -m promethean.pantheon.cli.core"
  },
  "module": "dist/index.js",
  "dependencies": {
    "@types/node": "^20.0.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "cheshire": "0.1.42",
    "fs-extra": "^11.0.0",
    "yaml": "^2.5.1",
    "amqplib": "^0.10.3",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@types/amqplib": "^0.10.5",
    "ava": "^5.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "shadow-cljs": "^2.28.17",
    "nbb": "^1.3.182",
    "rimraf": "^5.0.5",
    "c8": "^8.0.1",
    "eslint": "^8.57.0",
    "prettier": "^3.1.1"
  },
  "license": "GPL-3.0-only",
  "keywords": [
    "agents",
    "ai",
    "orchestration",
    "workflow",
    "context",
    "protocol",
    "promethean"
  ]
}