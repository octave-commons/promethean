{
  "name": "@promethean/discord",
  "version": "0.0.1",
  "license": "GPL-3.0-only",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json",
    "test": "pnpm run build && ava",
    "lint": "pnpm exec eslint .",
    "coverage": "pnpm run build && c8 ava",
    "format": "pnpm exec prettier --write ."
  },
  "dependencies": {
    "@discordjs/voice": "^0.18.0",
    "@promethean/agent": "workspace:*",
    "@promethean/effects": "workspace:*",
    "@promethean/embedding": "workspace:*",
    "@promethean/event": "workspace:*",
    "@promethean/legacy": "workspace:*",
    "@promethean/migrations": "workspace:*",
    "@promethean/persistence": "workspace:*",
    "@promethean/platform": "workspace:*",
    "@promethean/providers": "workspace:*",
    "@promethean/monitoring": "workspace:*",
    "@promethean/security": "workspace:*",
  "version": "0.0.1",
  "license": "GPL-3.0-only",
    "csv-stringify": "^6.5.2",
  "type": "module",
  "sideEffects": false,
    "discord.js": "^14.17.3",
    "dotenv": "^17.2.0",
    "libsodium-wrappers": "^0.7.13",
    "mongodb": "^6.17.0",
    "node-crc": "^1.3.2",
    "ollama": "^0.5.16",
    "prism-media": "^2.0.0-alpha.0",
    "wav": "^1.0.2",
    "ws": "^8.18.3"
  }
}
