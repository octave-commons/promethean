{
  "name": "@promethean/image-link-generator",
  "root": "packages/image-link-generator",
  "sourceRoot": "packages/image-link-generator/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -b",
        "cwd": "packages/image-link-generator"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tsconfig.json --noEmit",
        "cwd": "packages/image-link-generator"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec ava --config ../../config/ava.config.mjs",
        "cwd": "{projectRoot}"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint .",
        "cwd": "packages/image-link-generator"
      }
    }
  },
  "tags": ["scope:packages"]
}
