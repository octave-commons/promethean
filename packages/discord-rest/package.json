{
  "name": "@promethean/discord-rest",
  "version": "0.0.1",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "start": "node --enable-source-maps --experimental-specifier-resolution=node ./src/index.ts",
    "test": "pnpm run build && ava tests/**/*.test.js",
    "coverage": "pnpm run build && c8 ava tests/**/*.test.js",
    "build": "tsc -p tsconfig.json",
    "lint": "pnpm dlx @biomejs/biome lint .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@promethean/agent": "workspace:*",
    "@promethean/monitoring": "workspace:*",
    "@promethean/security": "workspace:*",
    "@promethean/platform": "workspace:*"
  }
}
