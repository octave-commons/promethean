{
  "name": "@promethean/opencode-session-manager",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "public/js/main.js",
  "exports": {
    ".": {
      "import": "./public/js/main.js",
      "require": "./public/js/main.js"
    },
    "./public/*": "./public/*",
    "./*": "./public/*"
  },
  "files": [
    "public"
  ],
  "scripts": {
    "build": "shadow-cljs release app",
    "clean": "rimraf public/js",
    "test": "pnpm run build && ava --config ../../config/ava.config.mjs",
    "lint": "pnpm exec eslint .",
    "coverage": "pnpm run build && c8 ava --config ../../config/ava.config.mjs",
    "dev": "shadow-cljs watch app",
    "server": "shadow-cljs server",
    "repl": "shadow-cljs cljs-repl app",
    "start": "node public/health.js",
    "start:dev": "concurrently \"npm run dev\" \"npm run start\"",
    "format": "pnpm exec prettier --write ."
  },
  "module": "public/js/main.js",
  "dependencies": {
    "@opencode-ai/sdk": "^0.15.2",
    "@promethean/ds": "workspace:*",
    "@promethean/utils": "workspace:*",
    "object-assign": "^4.1.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "scheduler": "^0.27.0"
  },
  "devDependencies": {
    "@promethean/test-utils": "workspace:*",
    "concurrently": "^8.2.2",
    "esbuild-register": "^3.5.0",
    "rimraf": "^5.0.5",
    "shadow-cljs": "^2.28.17"
  }
}
