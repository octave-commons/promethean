{
  "name": "opencode-session-manager",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/opencode-session-manager/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/js:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "command": "shadow-cljs release app",
        "cwd": "packages/opencode-session-manager"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/opencode-session-manager/**/*.{js,jsx,cljs}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/opencode-session-manager/ava.config.mjs",
        "passWithNoTests": true
      },
      "dependsOn": ["build"]
    }
  },
  "tags": ["scope:frontend", "type:ui", "platform:web"]
}
