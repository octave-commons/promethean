{
  "name": "@promethean/mcp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./github/sandboxes": {
      "types": "./dist/github/sandboxes/index.d.ts",
      "import": "./dist/github/sandboxes/index.js"
    },
    "./github/conflicts": {
      "types": "./dist/github/conflicts/index.d.ts",
      "import": "./dist/github/conflicts/index.js"
    },
    "./ollama": {
      "types": "./dist/ollama/index.d.ts",
      "import": "./dist/ollama/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "promethean-mcp-proxy": "dist/bin/proxy.js"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "pnpm run build && ava --config ../../config/ava.config.mjs",
    "lint": "pnpm exec eslint .",
    "coverage": "pnpm run build && c8 ava --config ../../config/ava.config.mjs",
    "dev": "tsx src/index.ts",
    "proxy": "tsx src/bin/proxy.ts"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/static": "^7.0.4",
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@promethean/discord": "workspace:*",
    "@promethean/kanban": "workspace:*",
    "@stryker-mutator/core": "^8.7.1",
    "fast-check": "^3.23.2",
    "fastify": "5.0.0",
    "fflate": "^0.8.2",
    "jsedn": "0.4.1",
    "minimatch": "^9.0.5",
    "type-fest": "^4.24.0",
    "uuid": "9.0.1",
    "zod": "^3.23.8",
    "zod-to-json-schema": "3.23.5"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "esmock": "^2.7.3",
    "nock": "^14.0.10",
    "rimraf": "6.0.1",
    "tsx": "4.19.2"
  },
  "license": "GPL-3.0-only"
}
