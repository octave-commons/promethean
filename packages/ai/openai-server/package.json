{
  "name": "@promethean/openai-server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "GPL-3.0-only",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dist/*": "./dist/*",
    "./static/*": "./static/*"
  },
  "files": [
