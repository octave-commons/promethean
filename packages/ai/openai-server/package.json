    "test": "pnpm run build && ava",
    "lint": "pnpm exec eslint ."
  },
  "dependencies": {
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/rate-limit": "^9.0.0",
    "@fastify/static": "8.2.0",
    "@fastify/swagger": "9.5.1",
    "@fastify/swagger-ui": "5.2.3",
    "bcryptjs": "^2.4.3",
    "fastify": "5.3.2",
    "jsonwebtoken": "^9.0.0",
    "sanitize-html": "^2.11.0"
  }
