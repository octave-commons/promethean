{
  "name": "@promethean/openai-server",
  "root": "packages/ai/openai-server",
  "sourceRoot": "packages/ai/openai-server/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run build",
        "cwd": "packages/ai/openai-server"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run typecheck",
        "cwd": "packages/ai/openai-server"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run test",
        "cwd": "{projectRoot}",
        "env": {
          "AVA_PROJECT_ROOT": "."
        }
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run lint",
        "cwd": "packages/ai/openai-server"
      }
    }
  },
  "tags": ["scope:packages"]
}
