{
  "name": "@promethean/chat-ui",
  "version": "1.0.0",
  "description": "ClojureScript chat UI for viewing and managing indexed conversations",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "shadow-cljs watch app",
    "build": "shadow-cljs release app",
    "clean": "shadow-cljs clean",
    "server": "shadow-cljs server",
    "repl": "shadow-cljs cljs-repl",
    "api": "ts-node src/server/index.ts",
    "start": "concurrently \"npm run api\" \"npm run dev\""
  },
  "dependencies": {
    "@promethean/persistence": "workspace:*",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "scheduler": "^0.27.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "concurrently": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shadow-cljs": "^2.25.8",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "shadow-cljs": {
    "source-paths": [
      "src"
    ],
    "dependencies": {
      "@promethean/database": "workspace:*",
      "@promethean/opencode-client": "workspace:*"
    }
  }
}
