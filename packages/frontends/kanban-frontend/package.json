{
  "name": "@promethean/kanban-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/frontend/index.cjs",
  "module": "dist/frontend/index.js",
  "types": "dist/frontend/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/frontend/index.d.ts",
      "import": "./dist/frontend/index.js",
      "require": "./dist/frontend/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "static"
  ],
  "scripts": {
    "build": "tsc",
    "build:shadow": "shadow-cljs release kanban-frontend",
    "watch:shadow": "shadow-cljs watch kanban-frontend",
    "clean": "rimraf dist dist/frontend",
    "typecheck": "tsc -p tsconfig.json",
    "test": "pnpm run build && ava",
    "lint": "pnpm exec eslint .",
    "coverage": "pnpm run build && c8 ava",
    "format": "pnpm exec prettier --write ."
  },
  "dependencies": {},
  "devDependencies": {
    "shadow-cljs": "2.28.17"
  }
}
