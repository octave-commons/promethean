{
  "name": "kanban-frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/frontends/kanban-frontend/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/workspace:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "command": "shadow-cljs release kanban-frontend",
        "cwd": "packages/frontends/kanban-frontend"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/frontends/kanban-frontend/**/*.{js,cljs,ts}"]
      }
    },
    "test": {
      "executor": "@nx/workspace:run-commands",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "command": "pnpm run test",
        "cwd": "packages/frontends/kanban-frontend"
      }
    }
  },
  "tags": ["scope:frontend", "type:ui"]
}
