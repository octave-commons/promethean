{
  "name": "@promethean/docops",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "ava --timeout=60s \"test/**/*.js\"",
    "doc:01-frontmatter": "tsx ./src/01-frontmatter.ts --dir ../../docs/unique --gen-model qwen3:4b",
    "doc:02-embed": "tsx ./src/02-embed.ts --dir ../../docs/unique --collection docs-cosine",
    "doc:03-query": "tsx ./src/03-query.ts --collection docs-cosine --force true",
    "doc:04-relate": "tsx ./src/04-relations.ts --docs-dir ../../docs/unique --doc-threshold 0.78 --ref-threshold 0.85",
    "doc:05-footers": "tsx ./src/05-footers.ts --dir ../../docs/unique --anchor-style block",
    "doc:06-rename": "tsx ./src/06-rename.ts --dir ../../docs/unique",
    "doc:all": "pnpm doc:01-frontmatter && pnpm doc:02-embed && pnpm doc:03-query && pnpm doc:04-relate && pnpm doc:05-footers && pnpm doc:06-rename",
    "doc:dev-ui": "tsx ./src/dev-ui.ts --dir ../../docs/unique --collection docs-cosine --port 3939"
  },
  "dependencies": {
    "@chroma-core/ollama": "^0.1.7",
    "@fastify/static": "^8.2.0",
    "@types/level": "^8.0.0",
    "abstract-level": "^3.1.0",
    "ava": "^6.4.1",
    "chromadb": "^3.0.14",
    "fastify": "^5.0.0",
    "gray-matter": "^4.0.3",
    "level": "^10.0.0",
    "ollama": "^0.5.17",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.5"
  }
}
