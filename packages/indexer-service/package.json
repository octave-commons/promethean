{
  "name": "@promethean/indexer-service",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "default": "./dist/main.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "default": "./dist/client.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "dev": "tsx watch src/main.ts",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "pnpm exec eslint src",
    "test": "pnpm run build && ava --config ../../config/ava.config.mjs",
    "coverage": "pnpm run build && c8 ava --config ../../config/ava.config.mjs"
  },
  "dependencies": {
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@promethean/indexer-core": "workspace:*",
    "@promethean/utils": "workspace:*",
    "fastify": "^5.3.2"
  }
}
