{
  "name": "@promethean/enso-agent-communication",
  "version": "1.0.0",
  "description": "Enso Protocol-based agent communication layer for coordinated AI agent collaboration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "ava",
    "coverage": "c8 ava",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@promethean/enso-protocol": "workspace:*",
    "@promethean/agent-orchestrator": "workspace:*",
    "zod": "^3.22.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "ava": "^5.0.0",
    "c8": "^8.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "ava": {
    "files": [
      "dist/tests/**/*.test.js"
    ],
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/promethean/promethean.git",
    "directory": "packages/enso-agent-communication"
  },
  "keywords": [
    "enso",
    "agent",
    "communication",
    "collaboration",
    "ai",
    "coordination"
  ],
  "author": "Promethean Team",
  "license": "GPL-3.0"
}